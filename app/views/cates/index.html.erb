<p id="notice"><%= notice %></p>

<h3>Category</h3>
<%= link_to 'New Category', new_cate_path %> | <%= link_to 'New Product', new_product_path %><br><br>


<table>
  <thead>
    <tr>
      <th></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cates.each do |cate| %>
      <tr>
        <td><%= cate.productname %></td>
        <td><%= link_to 'Show', cate %></td>
        <td><%= link_to 'Edit', edit_cate_path(cate) %></td>
        <td><%= link_to 'Destroy', cate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

